{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020", "DOM"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@alexandria/shared": ["src/types/alexandria-shared.d.ts"]
    },
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "types": []
  },
  "include": ["test-alfred-components.ts", "src/plugins/alfred/**/*.tsx", "src/types/**/*.d.ts"]
}