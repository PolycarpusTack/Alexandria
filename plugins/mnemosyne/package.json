{
  "name": "@alexandria/plugin-mnemosyne",
  "version": "0.1.0",
  "description": "Mnemosyne - Comprehensive Knowledge Management and Graph Visualization Plugin",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "extensionsToTreatAsEsm": [".ts"],
    "testEnvironment": "node",
    "roots": ["<rootDir>/src", "<rootDir>/tests"],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": ["ts-jest", {"useESM": true}]
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/types/**/*",
      "!src/database/migrations/**/*",
      "!src/index.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.ts"],
    "testTimeout": 10000,
    "verbose": true
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:prod": "tsc --build --verbose",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":80,\"functions\":85,\"lines\":85,\"statements\":85}}'",
    "test:unit": "jest src/**/*.test.ts",
    "test:integration": "jest src/tests/integration/",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "clean": "rm -rf dist",
    "prebuild": "pnpm run clean",
    "typecheck": "tsc --noEmit",
    "dev": "tsc -w",
    "migration:create": "typeorm migration:create",
    "migration:run": "typeorm migration:run -d ./src/database/data-source.ts",
    "migration:revert": "typeorm migration:revert -d ./src/database/data-source.ts",
    "server:dev": "nodemon --watch src --ext ts --exec ts-node src/index.ts"
  },
  "keywords": [
    "alexandria",
    "plugin",
    "knowledge-management",
    "graph-visualization",
    "semantic-search",
    "templates",
    "import-export",
    "relationships",
    "knowledge-graph",
    "ai-integration"
  ],
  "author": "Alexandria Platform Team",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-tooltip": "^1.1.3",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "fuse.js": "^7.0.0",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.460.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "react-markdown": "^9.0.1",
    "socket.io": "^4.6.2",
    "tailwind-merge": "^2.5.4",
    "typeorm": "^0.3.17",
    "uuid": "^11.0.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/express": "^4.17.21",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.17.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  }
}
