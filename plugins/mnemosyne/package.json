{
  "name": "@alexandria/plugin-mnemosyne",
  "version": "0.1.0",
  "description": "Mnemosyne - Comprehensive Knowledge Management and Graph Visualization Plugin",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:prod": "tsc --build --verbose",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":80,\"functions\":85,\"lines\":85,\"statements\":85}}'",
    "test:unit": "jest src/**/*.test.ts",
    "test:integration": "jest src/tests/integration/",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "clean": "rm -rf dist",
    "prebuild": "pnpm run clean",
    "typecheck": "tsc --noEmit",
    "dev": "tsc -w"
  },
  "keywords": [
    "alexandria",
    "plugin",
    "knowledge-management",
    "graph-visualization",
    "semantic-search",
    "templates",
    "import-export",
    "relationships",
    "knowledge-graph",
    "ai-integration"
  ],
  "author": "Alexandria Platform Team",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-tooltip": "^1.1.3",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "fuse.js": "^7.0.0",
    "handlebars": "^4.7.8",
    "lodash": "^4.17.21",
    "lucide-react": "^0.460.0",
    "react-markdown": "^9.0.1",
    "tailwind-merge": "^2.5.4",
    "uuid": "^11.0.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.12",
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "alexandria": ">=0.3.0"
  }
}