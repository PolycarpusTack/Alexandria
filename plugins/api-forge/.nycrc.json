{
  "all": true,
  "include": [
    "src/**/*.js",
    "index.js"
  ],
  "exclude": [
    "**/*.test.js",
    "tests/**",
    "node_modules/**",
    "coverage/**"
  ],
  "reporter": [
    "text",
    "html",
    "lcov",
    "json"
  ],
  "cache": true,
  "check-coverage": true,
  "lines": 80,
  "functions": 80,
  "branches": 80,
  "statements": 80,
  "watermarks": {
    "lines": [80, 95],
    "functions": [80, 95],
    "branches": [80, 95],
    "statements": [80, 95]
  }
}