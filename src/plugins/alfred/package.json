{
  "name": "@alexandria/plugin-alfred",
  "version": "2.0.0",
  "description": "ALFRED - AI Coding Assistant Plugin for Alexandria",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist",
    "prebuild": "pnpm run clean"
  },
  "keywords": [
    "alexandria",
    "plugin",
    "ai",
    "coding",
    "assistant",
    "alfred"
  ],
  "author": "Alexandria Team",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "mustache": "^4.2.0",
    "uuid": "^11.0.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@alexandria/(.*)$": "<rootDir>/../../$1"
    },
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/**/*.test.ts"
    ]
  }
}