{
  "name": "alexandria-platform",
  "version": "0.1.0",
  "description": "Modular AI-enhanced Customer Care & Services Platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "node scripts/multiplatform-build.js",
    "build:server": "node scripts/multiplatform-build.js --server-only",
    "build:client": "node scripts/multiplatform-build.js --client-only",
    "build:prod": "node scripts/multiplatform-build.js --bundle",
    "start": "node dist/index.js",
    "dev": "node scripts/dev-multiplatform.js",
    "dev:server": "npx ts-node-dev --respawn src/index.ts",
    "dev:client": "npx vite",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "clean": "node -e \"require('fs').rmSync('./dist', {recursive:true,force:true}); require('fs').rmSync('./build', {recursive:true,force:true})\"",
    "setup:db": "node scripts/setup-database.js"
  },
