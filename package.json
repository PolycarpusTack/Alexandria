{
  "name": "alexandria-platform",
  "version": "0.1.0",
  "description": "Modular AI-enhanced Customer Care & Services Platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "pnpm run build:packages && pnpm run build:server && pnpm run build:client",
    "build:packages": "pnpm run build --filter '@alexandria/*'",
    "build:server": "tsc --project tsconfig.server.json",
    "build:client": "vite build",
    "start": "node dist/index.js",
    "dev": "node dev-robust.js",
    "dev:npx": "node dev-npx.js",
    "dev:simple": "node dev-simple.js",
    "dev:alt": "node dev-start.js",
    "dev:concurrent": "concurrently \"pnpm run dev:server\" \"pnpm run dev:client\"",
    "dev:separate": "echo Please run 'pnpm run dev:server' and 'pnpm run dev:client' in separate terminals",
    "dev:server": "npx ts-node-dev --respawn src/index.ts",
    "dev:server:patched": "node ts-node-dev-patched.js --respawn src/index.ts",
    "dev:server:original": "ts-node-dev --respawn src/index.ts",
    "dev:client": "npx vite",
    "fix-rollup": "node fix-rollup.js",
    "test": "jest",
    "test:coverage": "jest --coverage --coverageReporters=lcov --testResultsProcessor=jest-sonar-reporter",
    "test:summary": "node scripts/test-summary-simple.js",
    "test:summary:save": "node scripts/test-summary-simple.js --save",
    "test:summary:verbose": "node scripts/test-summary-simple.js --verbose",
    "test:report": "node scripts/test-summary.js",
    "test:report:save": "node scripts/test-summary.js --save",
    "test:ai-models": "ts-node scripts/test-ai-models.ts",
    "test:ollama-compat": "ts-node scripts/check-ollama-compat.ts",
    "lint": "eslint --config eslint.config.js src --ext .ts,.tsx",
    "lint:fix": "eslint --config eslint.config.js src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "lint:ts": "tsc --noEmit",
    "quality:check": "pnpm run lint && pnpm run format:check && pnpm run lint:ts",
    "quality:fix": "pnpm run lint:fix && pnpm run format",
    "sonar:scan": "sonar-scanner",
    "sonar:test": "pnpm run test:coverage && pnpm run sonar:scan",
    "security:audit": "pnpm audit --audit-level moderate",
    "security:snyk": "snyk test --all-projects",
    "security:snyk-fix": "snyk fix",
    "security:monitor": "snyk monitor --all-projects",
    "security:full": "pnpm run security:audit && pnpm run security:snyk",
    "docs:generate": "ts-node scripts/generate-api-docs.ts",
    "docs:serve": "pnpm run docs:generate && http-server docs/api -p 8080 -o",
    "docs:validate": "ts-node scripts/validate-api-docs.ts",
    "check:deprecations": "node scripts/scan-deprecations.js",
    "fix:deprecations": "node scripts/fix-deprecations.js",
    "fix:concurrently": "node fix-concurrently.js",
    "dev:patched": "node start-dev-patched.js",
    "permissions": "ts-node src/cli/index.ts",
    "permissions:list": "ts-node src/cli/index.ts list",
    "permissions:validate": "ts-node src/cli/index.ts validate"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.17.0",
    "@radix-ui/react-alert-dialog": "^1.1.2",
    "@radix-ui/react-collapsible": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.1",
    "@radix-ui/react-scroll-area": "^1.2.1",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.1.3",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.4.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "file-type": "^21.0.0",
    "gray-matter": "^4.0.3",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.469.0",
    "multer": "^1.4.5-lts.1",
    "mustache": "^4.2.0",
    "pg": "^8.14.0",
    "react": "^18.3.1",
    "react-day-picker": "^9.4.8",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.2.0",
    "recharts": "^2.15.0",
    "styled-components": "^6.1.18",
    "tailwind-merge": "^2.5.5",
    "typeorm": "^0.3.21",
    "uuid": "^11.0.4",
    "winston": "^3.17.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@rollup/rollup-linux-x64-gnu": "^4.30.0",
    "@tailwindcss/postcss": "^4.1.8",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/chart.js": "^2.9.41",
    "@types/cors": "^2.8.17",
    "@types/csurf": "^1.11.5",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/joi": "^17.2.3",
    "@types/js-yaml": "^4.0.9",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/mustache": "^4.2.5",
    "@types/node": "^22.10.7",
    "@types/pg": "^8.11.10",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/semver": "^7.5.8",
    "@types/styled-components": "^5.1.34",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/tough-cookie": "^4.0.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "@vitejs/plugin-react": "^4.3.7",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "concurrently": "^9.1.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.18.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.3",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "js-yaml": "^4.1.0",
    "lint-staged": "^16.1.0",
    "postcss": "^8.5.3",
    "prettier": "^3.4.2",
    "rollup": "^4.30.0",
    "snyk": "^1.1293.1",
    "sonarqube-scanner": "^4.2.3",
    "style-loader": "^4.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tailwindcss": "^4.1.7",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.4",
    "url-loader": "^4.1.1",
    "vite": "^6.0.11",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.15.2",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "pnpm": {
    "overrides": {
      "dompurify@<3.2.4": ">=3.2.4",
      "jspdf@<3.0.1": ">=3.0.1",
      "cookie@<0.7.0": ">=0.7.0"
    }
  }
}