# Server
PORT=4000
NODE_ENV=development
JWT_SECRET=replace_with_your_secret_key
ENCRYPTION_KEY=replace_with_your_encryption_key
PLATFORM_VERSION=0.1.0
PLUGINS_DIR=./src/plugins

# Ollama
OLLAMA_BASE_URL=http://localhost:11434/api
DEFAULT_LLM_MODEL=llama2:8b-chat-q4

# Database
USE_POSTGRES=false # Set to true to use PostgreSQL
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=replace_with_your_postgres_password
POSTGRES_DB=alexandria
POSTGRES_SSL=false

# Logging
LOG_LEVEL=info
